{
	"suggest.triggerAfterInsertEnter": true,
	"suggest.noselect": false,
	"suggest.minTriggerInputLength": 2,
	"suggest.acceptSuggestionOnCommitCharacter": true,
	"suggest.snippetIndicator": "â–º",

	"tsserver.enable": true,
	"tsserver.formatOnType": false,
	"tsserver.enableJavascript": true,

	"typescript.validate.enable": true,
	"typescript.format.placeOpenBraceOnNewLineForFunctions": true,
	"typescript.suggest.completeFunctionCalls": true,
	"typescript.suggest.paths": true,
	"typescript.suggest.enabled": true,
	"typescript.suggest.autoImports": true,
	"typescript.updateImportsOnFileMove.enable": true,
	
         "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact"
  },
        
	"prettier.requireConfig": false,
	"prettier.tslintIntegration": true,
	"prettier.eslintIntegration": true,
	"prettier.stylelintIntegration": false,
	"prettier.statusItemText": "Prettier",
	"coc.preferences.formatOnSaveFiletypes": ["typescriptreact", "typescript", "javascript"],
	"diagnostic.messageTarget": "echo",
	"suggest.floatEnable": false,
	"signature.target": "echo",
	"coc.preferences.hoverTarget": "echo"
}
